{
	"manifest_version": 2,
	"version": "1.0",
	"name": "jQueryLoader",
	"description": "jQuery loader",
	"author": "CherryPics",

	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"loader.js"
			],
            "run_at" :"document_end"
		}
	]
}
